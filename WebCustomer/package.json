    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.0.3"
  },
  "browser": {
    "crypto": false
  },
  "prettier": {
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.scss",
        "options": {
          "singleQuote": false
        }
      }
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
