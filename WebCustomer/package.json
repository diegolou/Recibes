    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "ngx-bootstrap": "^6.2.0",
    "ngx-currency": "^2.5.2",
    "ngx-filesaver": "^11.0.0",
    "ngx-google-places-autocomplete": "^2.0.5",
    "ngx-intl-tel-input": "^3.0.5",
    "ngx-mask": "^11.1.4",
    "ngx-scrollbar": "^7.5.4",
    "ngx-webcam": "^0.3.2",
    "pdfmake": "^0.1.70",
    "ramda": "^0.27.1",
